@{
    ViewData["Title"] = "Dashboard Page";
}

@model List<Document>

<div class="text-center">
    <h1 class="display-6">Yüklenen Fotolar</h1>
</div>

<div class="text-center">
    @foreach (var item in Model)
    {
        <div class="row">
            <div class="text-center col-md-6 offset-md-3">
                <img class="mb-3 img-fluid" src="~/uploads/images/@item.StoragePath" alt="Önizleme" width="40%" />
                <br/>
            </div>
        </div>
    }
</div>
