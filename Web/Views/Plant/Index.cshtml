                    <p class="fw-bold fs-3 text-success">Kabukları</p>
                    <div id="barkCarousel" class="carousel slide" data-bs-ride="carousel">
                        <div class="carousel-inner">
                            @for (var i = 0; i < plantImages.Bark.Count; i++)
