@model List<Document>

@{
    <table class="table table-striped table-hover">
        <thead>

            <tr>
                <th scope="col">Id</th>
                <th scope="col">Title</th>
            </tr>
        </thead>

        <tbody>

            @foreach (var item in Model)
            {
                <tr>

                    <th scope="row">@item.Id</th>

                    <td>@item.Title</td>

                </tr>
            }
        </tbody>

    </table>
}
